<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project4.DonutsController">
   <children>
      <ComboBox fx:id="donutComboBox" layoutX="44.0" layoutY="64.0" prefHeight="25.0" prefWidth="185.0" promptText="Choose a donut type..." />
      <ListView fx:id="yeastDonutFlavors" layoutX="44.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" />
      <ListView fx:id="cakeDonutFlavors" layoutX="44.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" />
      <ListView fx:id="donutHoleFlavors" layoutX="44.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" />
      <Spinner fx:id="donutQuantity" layoutX="268.0" layoutY="125.0" prefHeight="25.0" prefWidth="65.0">
         <valueFactory>
            <SpinnerValueFactory.IntegerSpinnerValueFactory max="100" min="1" />
         </valueFactory>
      </Spinner>
      <ListView fx:id="selectedDonuts" layoutX="357.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="addDonut" layoutX="284.0" layoutY="163.0" mnemonicParsing="false" onAction="#addDonut" text="&gt;&gt;" />
      <Button fx:id="removeDonut" layoutX="284.0" layoutY="200.0" mnemonicParsing="false" onAction="#removeDonut" text="&lt;&lt;" />
      <Button fx:id="addDonutsToOrder" layoutX="468.0" layoutY="346.0" mnemonicParsing="false" text="Add To Order" />
      <Label layoutX="388.0" layoutY="47.0" text="Order Donuts">
         <font>
            <Font size="23.0" />
         </font></Label>
   </children>
</AnchorPane>
